export default class TabManager
{
	constructor()
	{
		this.index 	= 0;
		this.list	= [];
	}

	reset( list = [])
	{
		this.list 	= list;
		this.index 	= 0;
	}

	next()
	{
		this.index++;

		if ( this.list.length <= this.index )
			this.index = 0;
	}

	getCommand()
	{
		console.log( this.list );
		return this.list[ this.index ] || false;
	}
}